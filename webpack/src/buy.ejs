<% var path = '.' %>
<!doctype html>
<html>

<head>
	<title>お買い物リスト - week dinner</title>
	<meta name="Description" content="">
	<meta name="Keywords" content="">
	<meta http-equiv="content-type" content="text/html; charset=UTF-8">
	<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=yes">
	<link rel="shortcut icon" type="image/x-icon" href="<%= path %>/assets/img/favicon.ico">
</head>

<body>
	<%- include('./modules/header.ejs', {path}) %>

	<section class="wrapper">
		<div class="contents">
			<div class="contents-wrapper">
				<form action="" method="">
					<h2 class="contents-title">お買い物リスト</h2>
					<p class="contents-title_discription">
						チェックを入れるとお買い物リストから非表示になります
					</p>
					<table class="table_list js-material_table mb25"></table>
					<table class="table_list">
						<thead>
							<tr><th>表示するお買い物リスト</th></tr>
						</thead>
						<tbody>
							<tr>
								<td class="table_list-date_list">
									<% for (var i = 0; i < 7; i++) { %>
										<%- include('./modules/dateList.ejs', {dateNum: i}) %>
									<% } %>
								</td>
							</tr>
						</tbody>
					</table>
					
					<a href="<%= path %>/delete.html" class="text_link mt15">非表示にしたお買い物リスト一覧</a>
				</form>
			</div>
		</div>

		<%- include('./modules/footer.ejs') %>
	</section>

	<%- include('./modules/modal.ejs', {path}) %>
</body>
</html>